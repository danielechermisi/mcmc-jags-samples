#
var
   n, Y[n], p, R[n], theta, V[nV], pc, thetac
model{

  # Likelihood
  for( i in 1 : n) {
    Y[i] ~ dbern( p )
  }

  for( i in 1 : n) {
    R[i] ~ dexp( theta )
  }

  
  for( i in 1 : nV) {
    V[i] ~ dgamma( pc, thetac )
  }

  # Prior
  p ~ dunif( 0, 1 )
  theta ~ dgamma(2, 10)
  
  pc ~ dunif( 1, 50 )
  thetac ~ dgamma(2, 100)
}